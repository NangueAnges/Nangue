#include<stdio.h>
int main(int argc, char** argv){
unsigned int a, b, c, P,S;
printf("entrer la longueur du rectangle");
scanf("%d",&a);
printf("entrer la largeur du rectangle");
scanf("%d",&b);
P=(a+b)*2;
S=a*b;
printf("le périmètre du rectangle est %d et l'aire du rectangle est %d:",P,S);
return 0;
}


